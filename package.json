{
	"name": "vscode-ext-learning",
	"displayName": "vscode-ext-learning",
	"description": "Learning of vscode extension",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:halo.config.check",
		"onCommand:halo.config.reload",
		"onCommand:halo.post.list"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "halo.config.check",
				"title": "Halo configuration check"
			},
			{
				"command": "halo.config.reload",
				"title": "Halo configuration reload"
			},
			{
				"command": "halo.post.list",
				"title": "Halo post list"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"moment": "^2.24.0"
	}
}
